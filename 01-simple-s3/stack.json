{
  "Parameters": {
    "BucketName": {
      "Type": "String",
      "Description": "The name of the bucket to create",
      "AllowedPattern": ".+"
    }
  },
  "Resources": {
    "Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Ref": "BucketName" },
        "Tags": [
          {
            "Key": "source",
            "Value": "cfn-templates"
          }
        ]
      }
    }
  }
}
