{
  "MyLambdaFunction": {
    "Type": "AWS::Lambda::Function",
    "Properties": {
      "Code": {
        "ZipFile": {
          "Fn::Join": [
            "",
            [
              "exports.handler = function(event) {\n",
              "  return {\n",
              "    statusCode: 200,\n",
              "    body: JSON.stringify('Hello from Lambda!'),\n",
              "  };\n",
              "};\n"
            ]
          ]
        }
      },
      "Handler": "exports.handler",
      "Role": {
        "Fn::GetAtt": [
          "LambdaExecutionRole",
          "Arn"
        ]
      },
      "Runtime": "nodejs16.x"
    }
  }
}
